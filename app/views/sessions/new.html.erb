<h1>Log In</h1>

<div>
<%= form_with url: session_path do |form| %>
  <div class="field">
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>
  <div class="field">
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  <div class="submit">
    <%= form.submit "Log in", class: "btn" %>
  </div>
<% end %>
</div>
